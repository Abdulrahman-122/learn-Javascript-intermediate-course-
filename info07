solution;;
function isSumBigger(number1, number2, total) {    //added ) here
  const sum = number1 + number2;
  
  if (sum > total) {  // ; removed from here + replaced Numbersum with sum
    return true;
  } else {
    return false;
  }
}

// Should return true
console.log('isSumBigger(1, 3, 2) returns: ' + isSumBigger(1, 3, 2));

// Should return false
console.log('isSumBigger(1, 3, 5) returns: ' + isSumBigger(1, 3, 5));



